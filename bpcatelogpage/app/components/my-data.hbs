<pharbers-my-data-table
    allData={{@model}}
    {{did-insert this.registerListener}}
    {{will-destroy this.unregisterListener}}
	uploadToastBorder={{this.uploadToastBorder}}
	uploadTextStatus={{this.uploadTextStatus}}
	uploadText={{this.uploadText}}
	closeuploadToast={{this.closeuploadToast}}
	uploadLoadedSize={{this.uploadLoadedSize}}
	uploadFileSize={{this.uploadFileSize}}
	showProgress={{this.showProgress}}
></pharbers-my-data-table>
<input style="display: none;" type="file" name="" id="my-file" onchange={{action "uploadFiles" this}} accept="application/vnd.ms-excel,application/vnd.openxmlformats-officedocument.spreadsheetml.sheet,text/csv,.csv,.xlsx,.xls,.xlsm">